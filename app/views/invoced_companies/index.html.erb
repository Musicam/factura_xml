<h1>Listing invoced_companies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @invoced_companies.each do |invoced_company| %>
      <tr>
        <td><%= invoced_company.name %></td>
        <td><%= link_to 'Show', invoced_company %></td>
        <td><%= link_to 'Edit', edit_invoced_company_path(invoced_company) %></td>
        <td><%= link_to 'Destroy', invoced_company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

